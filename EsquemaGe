CREATE TABLESPACE ge_Ind
DATAFILE 'C:\app\sebas\oradata\dbprueba\geind01.dbf'
    SIZE 10M
    REUSE
    AUTOEXTEND ON
    NEXT 512k
    MAXSIZE 200M;



CREATE TABLESPACE ge_Data
    DATAFILE 'C:\app\sebas\oradata\dbprueba\gedata01.dbf'
    SIZE 10M 
    REUSE
    AUTOEXTEND ON
    NEXT 512k
    MAXSIZE 200M;

